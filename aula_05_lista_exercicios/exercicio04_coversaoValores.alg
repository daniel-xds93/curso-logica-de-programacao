algoritmo "semnome"
// Função :
// Autor :
// Data : 17/04/2020
// Seção de Declarações 
var
reais, valorDolar, conversao: real
inicio

ESCREVA("Quantos reais você tem R$ ")
LEIA(reais)

ESCREVA("Informe o valor do dólar R$ ")
LEIA(valorDolar)

conversao <- reais / valorDolar

ESCREVA("Em dólares você tem $ ", conversao:3:2)

fimalgoritmo
